{
  "name": "multi-tenant",
  "version": "1.0.0",
  "description": "An example of a multi tenant application with Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "_dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation && next dev --turbo",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:schema": "payload-graphql generate:schema",
    "generate:types": "payload generate:types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "seed": "npm run payload migrate:fresh",
    "status": "npm run payload migrate:status",
    "generate": "npm run payload migrate:create",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@payloadcms/db-postgres": "^3.2.2",
    "@payloadcms/live-preview-react": "^3.2.2",
    "@payloadcms/next": "^3.2.2",
    "@payloadcms/payload-cloud": "^3.2.2",
    "@payloadcms/plugin-form-builder": "^3.2.2",
    "@payloadcms/plugin-nested-docs": "^3.2.2",
    "@payloadcms/plugin-redirects": "^3.2.2",
    "@payloadcms/plugin-search": "^3.2.2",
    "@payloadcms/plugin-seo": "^3.2.2",
    "@payloadcms/richtext-lexical": "^3.2.2",
    "@payloadcms/storage-s3": "^3.2.2",
    "@payloadcms/ui": "^3.2.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "cssVariables": "link:@/cssVariables",
    "dotenv": "^8.2.0",
    "framer-motion": "^11.15.0",
    "graphql": "^16.9.0",
    "lucide-react": "^0.469.0",
    "next": "^15.0.0",
    "payload": "^3.2.2",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.4.1",
    "qs-esm": "7.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-syntax-highlighter": "^15.6.1",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "^3.2.2",
    "@payloadcms/graphql": "^3.2.2",
    "@swc/core": "^1.6.13",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.16.2",
    "typescript": "5.5.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}